@model ExamModel
@{
}
<div class="row">

    <div class="col-lg-12">
        <div class="card">
            <div class="form-group">
                <label> Lesson Name  </label>
                @Html.TextBoxFor(model => model.Exam.LessonId, new { @class = "form-control", placeholder = "Enter Lesson Name", required = "" })
                @Html.HiddenFor(model => model.Exam.Id)
            </div>

            <div class="form-group">
                <label>Exam Title </label>
                @Html.TextBoxFor(model => model.Exam.ExamName, new { @class = "form-control", placeholder = "Enter Exam Name", required = "" })
            </div>



        </div>
    </div>
</div>
